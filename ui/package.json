    "js-yaml": "^4.1.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.0.0",
    "prettier": "^3.1.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "sass": "1.54.4",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.9.5",
    "yaml-loader": "^0.8.0"
  },
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "license": "MIT"
}